{
  "name": "git-blog-editor",
  "version": "1.0.0",
  "description": "GitHub-based blog editor",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.ts",
    "start:ts": "ts-node server/index.ts",
    "dev": "nodemon server/index.ts",
    "dev:ts": "nodemon --exec ts-node server/index.ts",
    "build": "tsc",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "dev:full:ts": "concurrently \"npm run dev:ts\" \"npm run client\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "octokit": "^3.1.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
